{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      // ðŸ‘‡ Tell TypeScript to use the web fork whenever it sees "react-native-maps"
      "react-native-maps": ["node_modules/@teovilla/react-native-web-maps"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
